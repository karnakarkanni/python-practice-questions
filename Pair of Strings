a=input()
b=[]
for i in a:
    b.append(i)
i=0
while(i<len(b)-1):
    if b[i]==b[i+1]:
         b.remove(b[i])
         b.remove(b[i])
    else:
         i+=1
c=str()
for i in b:
    c+=i
print(c)

