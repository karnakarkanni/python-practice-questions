
def is_palindrome(n):
    return str(n) == str(n)[::-1]

def reverse_and_check_palindrome(num1, num2):
    for n in range(num1, num2 + 1):
        if is_palindrome(n):
            print(n)

num1 = int(input("Enter the first integer: "))
num2 = int(input("Enter the second integer: "))

reverse_and_check_palindrome(num1, num2)

     