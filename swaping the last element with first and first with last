n=list(map(int,input("ente").split(",")))
print(n)
b=len(n)
if b==len(n):
    temp=n[0]
    n[0]=n[b-1]
    n[b-1]=temp
    
    temp=n[1]
    n[1]=n[b-2]
    n[b-2]=temp
print(n)